{
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "Content object",
  "type": "object",
  "properties": {
    "layer": {
      "type": "string",
      "description": "The layer ID this content belongs to, in the `3DTILES_layers` `layers` dictionary."
    },
    "uri": {
      "type": "string",
      "description": "The URI to the content."
    },
    "mimeType": {
      "type": "string",
      "description": "The mimeType of the content."
    }
  },
  "required": ["uri", "mimeType"]
}